    },
    "order": {
      "type": "number"
    },
    "title": {
      "type": "string"
    },
    "keywords": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "created_at": {
      "type": "number"
    },
    "updated_at": {
      "type": "number"
    },
