{
  "name": "Article",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "order": {
      "type": "number"
    },
    "title": {
      "type": "string"
    },
    "keywords": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "text": {
      "type": "string"
    },
    "text_extension": {
      "type": "string"
    },
    "created_at": {
      "type": "number"
    },
    "updated_at": {
      "type": "number"
    },
    "link_text": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "articleImage": {
      "type": "hasOne",
      "model": "ArticleImage"
    },
    "category": {
      "type": "belongsTo",
      "model": "Category"
    }
  },
  "acls": [],
  "methods": {}
}
